<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p [%t] %c : %m%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="TRACE">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>